{
   "compilerOptions": {
      "module": "commonjs",
      "declaration": true,
      "removeComments": true,
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "allowSyntheticDefaultImports": true,
      "target": "es2017",
      "sourceMap": true,
      "outDir": "__build__",
      "incremental": true,
      "skipLibCheck": true,
      "strict": false,
      "jsx": "preserve",
      "baseUrl": "",
      "paths": {
         "~/*": ["./*"]
      }
   }
}
